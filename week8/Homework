install.packages("devtools")
library(usethis)
library(devtools)
#devtools::install_github("klutometis/roxygen")
library(roxygen2)
library(testthat)
library(tidyr)

#This function called convert_in_to_cm will convert inches into cm
# input inches into function


convert_in_to_cm <- function(inch) {
  centimeter <- (inch * 2.54)
  return(centimeter)
}


#This function is used to convert fahrenheit to celsius
#must input farenheit temperature into function


fahrenheit_to_celsius <- function(Far_Temp) {
  Cel_Temp <- (Far_Temp - 32) * 5 / 9
  return(Cel_Temp)
}
